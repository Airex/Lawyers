@model Lawyers.WebApp.Models.LawyersPageModel

<h1 class="text-center">@Model.SeoModel.H1</h1>
<div class="col-lg-12 btn-block">
    
    @foreach (var nav in Model.List)
    {
        <a class="btn btn-link btn-lg btn-block" href="@nav.Link">@nav.Label</a>
        <hr>
    }

</div>
<div class="col-lg-12 text-center fullmin">
    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <!-- adapt nav -->
    <ins class="adsbygoogle"
         style="display:block"
         data-ad-client="ca-pub-9943820082472623"
         data-ad-slot="9533568357"
         data-ad-format="auto"></ins>
    <script>
        (adsbygoogle = window.adsbygoogle || []).push({});
    </script>
</div>
@if (Model.ShowLawyers)
{
    await Html.RenderPartialAsync("LawyersAddon", Model);
}

<div class="block">
    @Model.SeoModel.Block
</div>